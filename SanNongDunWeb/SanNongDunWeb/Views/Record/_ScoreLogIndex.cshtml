@model  DataModel.RequestModel.Page<DataModel.LiuViewModel.ScoreViewModel>

@if (Model.Data != null)
{
    foreach (var item in Model.Data)
    {
      
        
        {
            <tr>
                <td width="10%">@item.ScoreLogId</td>
                <td width="0%"></td>

                <td width="20%">@Common.Auxiliary.ConvertMobilePhone(item.UserName)</td>
                <td width="0%"></td>
                <td width="10%">@item.LogCount</td>
                <td width="0%"></td>
                <td width="30%"><time>@item.CreateTime.ToString("yyyy/MM/dd HH:mm:ss")</time></td>
                <td width="0%"></td>
                <td width="20%">@item.Remark</td>
            </tr>
        }
    }
}